{
  "name": "Go back with backspace",
  "description": "Restore the backspace key shortcut to navigate to the previous page.",
  "author": "Namit Juneja",
  "version": "0.3.2",
  "manifest_version": 2,
  "minimum_chrome_version": "52",
  "content_scripts": [
      {
        "matches": [ "<all_urls>"],
        "all_frames": true,
        "run_at": "document_start",
        "js": ["content.js"]
      }
    ],
  "icons": {"128": "icon.png" }
}